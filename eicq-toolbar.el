(defvar eicq-log-toolbar
  '([eicq-password-icon
     eicq-toolbar-change-password t "Change password"]
    [eicq-send-message-here-icon
     eicq-toolbar-send-message-here t "Send message here"]
    [eicq-send-message-around-icon
